# ==================== 优化新增配置 ====================
# 内核安全加固
CONFIG_KERNEL_SECCOMP=y
CONFIG_KERNEL_CC_STACKPROTECTOR_REGULAR=y
CONFIG_KERNEL_SLAB_FREELIST_RANDOM=y
CONFIG_KERNEL_SLAB_FREELIST_HARDENED=y

# 编译优化
CONFIG_TARGET_OPTIMIZATION="-Os -pipe -mcpu=cortex-a55 -mtune=cortex-a55"
CONFIG_USE_MKLIBC=y

# 网络性能优化
CONFIG_KERNEL_NET_IPV6_NDISC_NODETYPE=y
CONFIG_KERNEL_IP_MULTICAST=y
CONFIG_KERNEL_IP_MROUTE=y
CONFIG_KERNEL_NET_IPGRE=y
CONFIG_KERNEL_NET_IPGRE_BROADCAST=y

# 文件系统支持增强
CONFIG_PACKAGE_kmod-fs-btrfs=y
CONFIG_PACKAGE_kmod-fs-exfat=y
CONFIG_PACKAGE_kmod-fs-ntfs3=y
CONFIG_PACKAGE_kmod-fs-autofs4=y

# 内核模块压缩（减少固件大小）
CONFIG_KERNEL_MODULE_COMPRESS_XZ=y

# 内存管理优化
CONFIG_KERNEL_SLUB_DEBUG=n
CONFIG_KERNEL_DEBUG_PAGEALLOC=n

# 移除调试符号（减少固件大小）
CONFIG_STRIP_KERNEL_EXPORTS=y
CONFIG_USE_SSTRIP=y

# 固件压缩优化
CONFIG_TARGET_IMAGES_GZIP=y
CONFIG_TARGET_ROOTFS_SQUASHFS=y

# 内核配置优化
CONFIG_KERNEL_DEBUG_KERNEL=n
CONFIG_KERNEL_DEBUG_INFO=n
